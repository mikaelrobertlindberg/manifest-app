{
  "cli": {
    "version": ">= 16.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "ios": {
        "buildType": "release"
      }
    },
    "production-apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "production-aab": {
      "android": {
        "buildType": "app-bundle"
      }
    },
    "ios-beta": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "buildType": "release",
        "credentialsSource": "local"
      },
      "env": {
        "EXPO_NO_CAPABILITY_SYNC": "1"
      }
    },
    "ios-beta-managed": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "buildType": "release",
        "credentialsSource": "remote"
      }
    },
    "ios-production": {
      "distribution": "store",
      "ios": {
        "credentialsSource": "local"
      }
    }
  },
  "submit": {
    "production": {},
    "ios-beta": {
      "ios": {
        "ascApiKeyPath": "./secrets/AuthKey.p8",
        "ascApiKeyId": "REPLACE_WITH_YOUR_KEY_ID",
        "ascApiKeyIssuerId": "REPLACE_WITH_YOUR_ISSUER_ID"
      }
    }
  }
}
